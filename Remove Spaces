class Solution
{
   
    String modify(String S)
    {
        int l=S.length();
        String mod="";
        for(int i=0;i<l;i++)
        {
            if(S.charAt(i)==' ')
            {
                continue;
            }
            else{
                mod=mod+Character.toString(S.charAt(i));
            }
        }
        return mod;
    }
}
